{
    "manifest_version": 3,
    "name": "ChatGPT Table of Contents (Unofficial)",
    "version": "1.0",
    "author": "Nodevantage",
    "description": "Floating table of contents for ChatGPT conversations.",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": [
          "https://chat.openai.com/*",
          "https://chatgpt.com/*",
          "https://chat.com/*"
        ],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ],
    "icons": {
      "16": "./icons/16.png",
      "32": "./icons/32.png",
      "48": "./icons/48.png",
      "128": "./icons/128.png"
    },
    "homepage_url": "https://github.com/nodevantage/ChatGPT-Table-of-Contents"
  }